    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/generate-realistic-data.ts",
    "db:seed:realistic": "tsx scripts/generate-realistic-data.ts",
    "android:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-android.ps1",
    "android:build": "node scripts/build-android.js",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:check": "powershell -ExecutionPolicy Bypass -File scripts/check-requirements.ps1",
    "android:install": "powershell -ExecutionPolicy Bypass -File scripts/install-requirements.ps1",
    "android:download-java": "powershell -ExecutionPolicy Bypass -File scripts/download-java.ps1",
