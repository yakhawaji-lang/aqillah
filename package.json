    "android:fix-path": "powershell -ExecutionPolicy Bypass -File scripts/verify-and-fix-path.ps1",
    "android:complete-setup": "powershell -ExecutionPolicy Bypass -File scripts/complete-setup-auto.ps1",
    "android:auto-install": "powershell -ExecutionPolicy Bypass -File scripts/install-all-requirements.ps1",
    "android:install-choco": "powershell -ExecutionPolicy Bypass -File scripts/install-chocolatey.ps1",
    "setup:google-maps": "powershell -ExecutionPolicy Bypass -File scripts/setup-google-maps.ps1",
