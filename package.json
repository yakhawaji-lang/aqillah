{
  "name": "aqillah",
  "version": "1.0.0",
  "description": "نظام وطني ذكي لتحليل الازدحام المروري - عَقِلْها",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:android": "cross-env BUILD_FOR_ANDROID=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:win": "powershell -Command \"if (Test-Path .next) { Remove-Item -Recurse -Force .next }; if (Test-Path node_modules\\.cache) { Remove-Item -Recurse -Force node_modules\\.cache }\"",
